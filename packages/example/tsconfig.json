{
  "compilerOptions": {
    "module": "amd",
    "moduleResolution": "node",
    "target": "ES2019",
    "strict": true,
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "useDefineForClassFields": true,
    "jsx": "react",
    "baseUrl": ".",
    "noEmit": true
  },
  "include": ["./**/*"],
  "exclude": ["node_modules"]
}
